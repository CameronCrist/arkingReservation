{
  "name": "private-parking-reservation",
  "version": "2.0.0",
  "description": "Private Parking Reservation System - Privacy-preserving parking resource allocation based on Zama FHE technology with Gateway integration and Fail-Closed Design",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "test": "npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "node": "npx hardhat node",
    "node:fhevm": "npx hardhat node --network fhevm",
    "start": "npx http-server public -p 3000 -c-1 --cors",
    "typechain": "npx hardhat typechain",
    "size": "npx hardhat size-contracts",
    "clean": "npx hardhat clean",
    "verify": "npx hardhat run scripts/verify.js --network sepolia",
    "interact": "npx hardhat run scripts/interact.js --network sepolia",
    "simulate": "npx hardhat run scripts/simulate.js --network sepolia",
    "check": "node scripts/check-setup.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint \"contracts/**/*.sol\" --fix",
    "format:check": "prettier --check \"contracts/**/*.sol\" \"test/**/*.js\"",
    "format": "prettier --write \"contracts/**/*.sol\" \"test/**/*.js\"",
    "security:check": "npm audit && npm run lint:sol",
    "security:slither": "slither . --detect reentrancy-eth,reentrancy-no-eth,reentrancy-benign",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "keywords": ["fhe", "zama", "parking", "privacy", "blockchain", "reservation", "gateway", "fhevm"],
  "author": "FHE Developer",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-security": "^1.7.1",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.45",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^4.0.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.5",
    "typechain": "^8.3.2",
    "typescript": "^5.2.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "dotenv": "^16.3.1"
  }
}